{% extends 'base.html.twig' %}

{% block title %}{{ 'Jurisdiction'|trans}}{% endblock %}

{% block body %}
    <div class="container mt-4">
    <div class="text-primary" style="font-weight: bold;font-size: 4rem;"><h1>{{ 'Jurisdiction'|trans}}</h1></div>

        <table class="table">
            <tbody>
                <tr>
                    <th>Id</th>
                    <td>{{ jurisdiction.id }}</td>
                </tr>
                <tr>
                    <th>{{ 'Name'|trans}}</th>
                    <td>{{ jurisdiction.name }}</td>
                </tr>
            </tbody>
        </table>

        <div class="text-right">
            <a href="{{ path('jurisdiction_index') }}" class="btn btn-primary">{{ 'Back to list'|trans}}</a>
        </div>
    </div>
{% endblock %}
