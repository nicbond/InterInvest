{% extends 'base.html.twig' %}

{% block title %}Jurisdiction index{% endblock %}

{% block body %}
    <h1>Jurisdiction index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for jurisdiction in jurisdictions %}
            <tr>
                <td>{{ jurisdiction.id }}</td>
                <td>{{ jurisdiction.name }}</td>
                <td>
                    <a href="{{ path('jurisdiction_show', {'id': jurisdiction.id}) }}">show</a>
                    <a href="{{ path('jurisdiction_edit', {'id': jurisdiction.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('jurisdiction_new') }}">Create new</a>
{% endblock %}
