<form method="post" action="{{ path('jurisdiction_delete', {'id': jurisdiction.id}) }}" onsubmit="return confirm('Etes vous sur de vouloir supprimer?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ jurisdiction.id) }}">
    <button class="btn btn-danger">{{ 'Delete'|trans}}</button>
</form>
